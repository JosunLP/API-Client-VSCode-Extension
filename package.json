{
  "name": "pulse-api-client",
  "publisher": "JosunLP",
  "displayName": "Pulse API Client",
  "icon": "icons/images/icon.png",
  "description": "Simple and intuitive API Client made into a VSCode extension.  A fork from REST API Client extension.",
  "version": "2.0.0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JosunLP/API-Client-VSCode-Extension/issues"
  },
  "author": {
    "name": "Jonas Pfalzgraf",
    "email": "support@josunlp.de"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JosunLP/API-Client-VSCode-Extension"
  },
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "api",
    "http",
    "REST",
    "client",
    "postman",
    "request",
    "response",
    "api-client"
  ],
  "activationEvents": [
    "onCommand:rest-api-client.newRequest",
    "onView:rest-api-client.collectionMenu"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rest-api-client.newRequest",
        "category": "Start",
        "title": "New Request"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "pulse-api-client",
          "title": "Pulse API Client",
          "icon": "icons/svg/httpIcon.svg"
        }
      ]
    },
    "views": {
      "pulse-api-client": [
        {
          "type": "webview",
          "id": "pulse-api-client.collectionMenu",
          "name": "Pulse API Client",
          "contextualTitle": "Pulse API Client",
          "icon": ""
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "dev": "npm run build:extension -- --watch & npm run build:webview -- --watch",
    "build:extension": "vite build --config vite.config.extension.ts",
    "build:webview": "vite build --config vite.config.webview.ts",
    "package": "npm run build:extension && npm run build:webview",
    "compile": "vsce package",
    "test-compile": "tsc -p ./",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "pretest": "npm run lint",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/postman-collection": "^3.5.11",
    "@vitejs/plugin-react": "^5.1.1",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "mocha": "^11.7.5",
    "path-browserify": "^1.0.1",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@types/react": "^19.2.6",
    "@types/serialport": "^8.0.5",
    "@types/styled-components": "^5.1.36",
    "@types/vscode": "^1.106.1",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "form-data": "^4.0.5",
    "postman-code-generators": "^2.1.0",
    "postman-collection": "^5.2.0",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-spinners": "^0.17.0",
    "serialport": "^13.0.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "use-debounce": "^10.0.6",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  }
}
